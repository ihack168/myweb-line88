{
  "redirects": [
    {
      "source": "/:year(\\d{4})/:month(\\d{1,2})/:slug*.html",
      "has": [
        {
          "type": "host",
          "value": "www.line88.tw"
        }
      ],
      "destination": "https://blog.line88.tw/:year/:month/:slug*.html",
      "permanent": true
    }
  ]
}